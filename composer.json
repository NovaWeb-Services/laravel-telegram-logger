{
    "name": "nwservices/laravel-telegram-logger",
    "version": "2.0.0",
    "description": "A Laravel package that monitors laravel.log and sends Telegram notifications when errors occur",
    "keywords": [
        "laravel",
        "telegram",
        "logger",
        "notifications",
        "log-monitor",
        "error-handling"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nova",
            "email": "nova@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "NWServices\\TelegramLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NWServices\\TelegramLogger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NWServices\\TelegramLogger\\TelegramLoggerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
