{
    "name": "nova/laravel-telegram-logger",
    "description": "A Laravel package for sending Telegram notifications when errors are logged",
    "keywords": [
        "laravel",
        "telegram",
        "logger",
        "notifications",
        "monolog",
        "error-handling"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nova",
            "email": "nova@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "monolog/monolog": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Nova\\TelegramLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nova\\TelegramLogger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nova\\TelegramLogger\\TelegramLoggerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
